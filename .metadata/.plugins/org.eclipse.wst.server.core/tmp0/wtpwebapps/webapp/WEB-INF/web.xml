<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CenovnikServlet</servlet-name>
  	<display-name>CenovnikServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.CenovnikServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RezervacijeServlet</servlet-name>
  	<display-name>RezervacijeServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.RezervacijeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CenovnikServlet</servlet-name>
  	<url-pattern>/CenovnikServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RezervacijeServlet</servlet-name>
  	<url-pattern>/RezervacijeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
</web-app>
